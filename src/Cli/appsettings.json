{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Error"
            }
        },
        "Using": [
            "Serilog.Sinks.File",
            "Serilog.Formatting.Compact"
        ],
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "rollingInterval": "Day",
                    "path": "logs/poo-.log"
                }
            }
        ],
        "Properties": {
            "Application Name": "TaskTitan"
        }
    }
}
